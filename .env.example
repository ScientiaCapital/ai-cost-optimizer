# Multi-LLM Router - Environment Configuration
# Enable providers by adding their API keys

# OpenRouter (40+ models in one API)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Direct Provider APIs
ANTHROPIC_API_KEY=sk-ant-your-key-here
GOOGLE_API_KEY=your-google-api-key
CEREBRAS_API_KEY=your-cerebras-key
DEEPSEEK_API_KEY=your-deepseek-key
HUGGINGFACE_API_KEY=hf_your-key-here

# Local Ollama (free, runs on your machine)
OLLAMA_ENABLED=false
OLLAMA_BASE_URL=http://localhost:11434

# Budget Settings
DEFAULT_MONTHLY_BUDGET=100.00

# Database
DATABASE_URL=sqlite:///./optimizer.db
